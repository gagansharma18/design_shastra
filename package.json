{
  "name": "design_shastra",
  "version": "1.0.0",
  "description": "Architech and design",
  "author": "Gagan sharma",
  "repository": "https://github.com/gagansharma18/design_shastra",
  "license": "ISC",
  "dependencies": {
    "animejs": "^2.2.0",
    "imagesloaded": "^4.1.4",
    "masonry-layout": "^4.2.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "raf": "^3.4.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-load-script": "0.0.6",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.0.14"
  },
  "devDependencies": {},
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o public/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build"
  },
  "homepage": "http://localhost:3000/"
}
